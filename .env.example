# Komodo MCP Server Configuration
# Copy this file to .env and fill in your values

# URL of your Komodo Core instance (required)
KOMODO_URL=https://komodo.example.com

# API key for authentication (required)
# Generate at: Komodo UI > Settings > API Keys
KOMODO_API_KEY=your-api-key-here

# API secret for authentication (required)
KOMODO_API_SECRET=your-api-secret-here

# Access tier: "read-only", "read-execute", or "full" (default: full)
# KOMODO_ACCESS_TIER=full

# Optional: Enable debug logging
# DEBUG=true
